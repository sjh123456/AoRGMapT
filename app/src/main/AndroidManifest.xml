            android:exported="true" />
        <activity
            android:name="com.Acquisition.WasteDisposalActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.ReclamationActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.AchievementAcceptanceActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.FieldAcceptanceActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.QualityTestingActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.FracturingTestActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.CoreDescriptionActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.SiteConstructionInputCollectionContentActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.SiteConstructionInputSiteDailyActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.FieldConstructionLoggingExplainActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.FieldConstructionLoggingSiteDailyActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.SiteConstructionWellDrillingActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.CommencementAcceptanceActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.WellSitePreparationActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.WellLocationDeterminationActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

