        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.Acquisition.LocalDispatchingRouteActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.GeophysicalGeochemicalExplorationActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.AnalysisAssayActivity"
            android:exported="true" />
        <activity
            android:name="com.Acquisition.PieChartActivity"
